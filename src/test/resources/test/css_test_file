    <script type="text/javascript">
        var divShow = document.getElementById('backtop');
        divShow.onclick = function () {
            document.documentElement.scrollTop = 0;
            document.body.scrollTop = 0;
        }
        function goTop() {
            var wtop = document.documentElement.scrollTop || document.body.scrollTop;
            if (!!window.ActiveXObject && !window.XMLHttpRequest) {
                var topOffset = 90;
                document.getElementById("gotop").style.top = (wtop + parseInt(document.documentElement.clientHeight) - topOffset) + "px";
            }
            if (wtop > 0) {
                document.getElementById("backtop").style.display = "";
            } else {
                document.getElementById("backtop").style.display = "none";
            }
        }
        window.setInterval("goTop()", 100);
    </script>

    <script src="/JavaScript/AutoAjax.js" type="text/javascript"></script>
    <script type="text/javascript">
       
        //选择城市
        function SelectCity() {
            var url = location.href;            
            url = url.substring(url.lastIndexOf('/'), url.lastIndexOf('-'));
            var arrUrl = url.split('-');
            arrUrl[1] = document.getElementById("selectCity").value;
            url = "/2sc";
            for (var i = 0; i < arrUrl.length; i++) {
                url += arrUrl[i] + "-";
            }
            location.href = url + "1.html";
        }
        //精图
        function CheckBoxClick(index) {
            var url = location.href;
            url = url.substring(url.lastIndexOf('/'), url.lastIndexOf('-'));
            var arrUrl = url.split('-');
            if (arrUrl[index] == 1) {
                arrUrl[index] = 0;
            }
            else {
                arrUrl[index] = 1;
            }
            url = "/2sc";
            for (var i = 0; i < arrUrl.length; i++) {
                url += arrUrl[i] + "-";
            }
            location.href = url + "1.html";
        }
        //排序
        function SelectStoreClick(val) {
            var url = location.href;
            url = url.substring(url.lastIndexOf('/'), url.lastIndexOf('-'));
            var arrUrl = url.split('-');
            arrUrl[13] = val;
            url = "/2sc";
            for (var i = 0; i < arrUrl.length; i++) {
                url += arrUrl[i] + "-";
            }
            location.href = url + "1.html";
        }
        //设置排序
        function SetselectStore() {
            var obj = document.getElementById("selectStore");
            if (obj != null) {
                var url = location.href;
                url = url.substring(url.lastIndexOf('/'), url.lastIndexOf('-'));
                var arrUrl = url.split('-');
                if (arrUrl.length > 10) {
                    obj.value = arrUrl[13];
                }
                else {
                    obj.value = 0;
                }
            }
        }
        SetselectStore();
    </script>

<script type="text/javascript">
    function fnH() {
        var ftree = document.getElementById('frame_tree');
        var fleft = document.getElementById('frame_left');
        var windowh = document.documentElement.clientHeight;
        ftree.style.height = (windowh - 151) + "px";    
        fleft.style.height = windowh + "px";
    }
    if (window.addEventListener) {
        window.addEventListener("load", fnH, false);
        window.addEventListener("resize", fnH, false);
    } else if (window.attachEvent) {
        window.attachEvent("onload", fnH);
        window.attachEvent("onresize", fnH);
    }
</script>

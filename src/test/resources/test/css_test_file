        <script type="text/javascript" language="javascript">
            function onSearch() {
                var val = document.getElementById("souform").getElementsByTagName("input")[0].value;
                val = val.replace(/(^\s*)(\s*$)/g, "");
                val = val.replace(/(<|>)/g, "");
                if (val != "" && val != "请输入品牌车型") {
                    document.getElementById("souform").getElementsByTagName("input")[0].value = val;
                    document.getElementById("souform").submit();
                }
            }
            function acOnFocus() {
                var script = document.createElement("script");
                script.src = "http://sou.autohome.com.cn/js/autocomplate_newcarprice.js?v10";
                script.onreadystatechange = script.onload = function () {
                    if (!this.readyState || this.readyState == "loaded" || this.readyState == "complete") {
                        if (!this.readyState || this.readyState == "loaded" || this.readyState == "complete")
                            Sou.Autocomplate.AddAutocomplate(document.getElementById("souform").getElementsByTagName("input")[0]);
                    }
                }
                document.getElementsByTagName("head")[0].appendChild(script);
                document.getElementById("souform").getElementsByTagName("input")[0].onfocus = "";
            }
            document.getElementById("souform").getElementsByTagName("input")[0].onfocus = acOnFocus;
            function acOnClick() {
                var obj = document.getElementById("souform").getElementsByTagName("input")[0];
                if (obj.value == "请输入品牌车型") {
                    obj.value = "";
                }
            }
            function acOnBlur() {
                var obj = document.getElementById("souform").getElementsByTagName("input")[0];
                if (obj.value == "") {
                    obj.value = "请输入品牌车型";
                }
            }
    </script>
